package main

import "fmt"

func main() {
	var idade int
	fmt.Print("Qual a sua idade?")
	fmt.Scan(&idade)
	if idade < 9 {
		fmt.Println("Vc faz parte da classificação mirim")
	} else if idade > 9 && idade < 13 {
		fmt.Println("Vc faz parte da classificação infantil")
	} else if idade > 13 && idade < 17 {
		fmt.Println("Vc faz parte da classificação juvenil")
	} else if idade >= 18 {
		fmt.Println("Vc faz parte da classificação adulto")
	}
}
